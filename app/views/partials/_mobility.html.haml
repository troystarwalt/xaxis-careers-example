.container.mobility.timeline
  %section.timelines-selector
    .row
      .col-xs-4.text-center.timeline-picture-selector
        =image_tag '//unsplash.it/400/600', class: 'img-responsive active'
        %h4 Employee's Name
      .col-xs-4.text-center.timeline-picture-selector
        =image_tag '//unsplash.it/400/600', class: 'img-responsive'
        %h4 Other Employees Name
      .col-xs-4.text-center.timeline-picture-selector
        =image_tag '//unsplash.it/400/600', class: 'img-responsive'
        %h4 Last Employees Name
