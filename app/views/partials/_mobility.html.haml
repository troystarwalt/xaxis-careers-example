.container.mobility.timeline
  %section.timelines-selector
    .row
      .col-xs-4.text-center.timeline-picture-selector
        =image_tag '//robohash.org/set_set2/bgset_bg1/123?size=500x500', class: 'img-responsive active'
        %h4 Employee 1
      .col-xs-4.text-center.timeline-picture-selector
        =image_tag '//robohash.org/set_set2/bgset_bg1/1?size=500x500', class: 'img-responsive'
        %h4 Employee 2
